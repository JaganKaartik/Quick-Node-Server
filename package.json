{
  "name": "quick-node",
  "version": "2.0.0",
  "description": "A template for creating a Express RESTful API server.",
  "main": "index.js",
  "repository": "https://github.com/MyCloudle/Quick-Node",
  "author": "JaganKaartik <kaartikjagan@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "NODE_ENV=production node dist/index",
    "dev": "NODE_ENV=development nodemon dist/index",
    "build": "tsc",
    "lint": "eslint '*/**/*.ts'",
    "lint:fix": "eslint '*/**/*.ts' --fix",
    "prettier": "prettier '*/**/*.ts' --write --color"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "yarn prettier",
      "yarn lint"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.8",
    "@types/passport": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "mongoose": "^5.10.11",
    "passport": "^0.4.1",
    "passport-google-oauth2": "^0.2.0"
  }
}
